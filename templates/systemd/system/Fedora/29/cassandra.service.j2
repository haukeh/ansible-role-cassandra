{{ ansible_managed | comment }}

[Unit]
Description=Cassandra Service

[Service]
Type=forking
User=cassandra
Group=cassandra
ExecStart=/usr/sbin/cassandra
WorkingDirectory=/var/lib/cassandra
Restart=always
RestartSec=60
TimeoutStopSec=60
TimeoutStartSec=60
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target
